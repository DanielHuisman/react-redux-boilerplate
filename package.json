{
  "name": "@danielhuisman/react-redux-boilerplate",
  "description": "Boilerplate for React + Redux + Webpack + Hot Loader. Highly opinionated as it's intended for personal use.",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)",
  "repository": "https://github.com/DanielHuisman/react-redux-boilerplate.git",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "hotloader",
    "boilerplate"
  ],
  "scripts": {
    "lint": "eslint --color src",
    "clean": "rimraf dist",
    "build": "yarn run clean && yarn run build:webpack && yarn run build:static && yarn run build:server",
    "build:webpack": "cross-env NODE_ENV=production babel-node ./node_modules/.bin/webpack --config webpack.production.js",
    "build:static": "copyfiles static/* dist",
    "build:server": "cross-env NODE_ENV=production babel src/server --out-dir dist/server --source-maps",
    "dev": "babel-node src/server/dev.js",
    "start": "cross-env NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "dot": "^1.1.1",
    "dotenv": "^4.0.0",
    "history": "^4.6.1",
    "koa": "^2.2.0",
    "koa-convert": "^1.2.0",
    "koa-router": "next",
    "koa-static-server": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^4.0.0",
    "debug": "^2.6.3",
    "eslint": "^3.18.0",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "koa-webpack-middleware": "^1.0.3",
    "react-hot-loader": "3.0.0-alpha.6",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
